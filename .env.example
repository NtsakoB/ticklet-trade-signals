# === Background Runner (order requested) ===
TICKLET_BG_ENABLED=true
TICKLET_BG_INTERVAL_SEC=60
TICKLET_SYMBOLS=any
TICKLET_TIMEFRAMES=15m,30m,1h,1d
TICKLET_SUPABASE_URL=
TICKLET_SUPABASE_ANON_KEY=
TICKLET_SUPABASE_SCHEMA=public

# === Frontend Supabase Configuration ===
# Get these values from your Supabase project dashboard
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_SUPABASE_PROJECT_ID=your_project_id_here

# === Backend Supabase Configuration ===
# Service role key for backend operations (different from anon key)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# === Persistent Disk (Render absolute paths) ===
TICKLET_DATA_DIR=/var/data/ticklet
TICKLET_MODELS_DIR=/var/data/ticklet/models
TICKLET_LOGS_DIR=/var/data/ticklet/logs

# === AI/ML Configuration ===
# Enable AI/ML features in backtesting
TICKLET_ML_ENABLED=true

# === Binance API (for backtesting data) ===
# No API key needed for public market data endpoints

# === OpenAI ===
TICKLET_OPENAI_KEY=sk-REPLACE_ME
TICKLET_OPENAI_MODEL=gpt-5-mini
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini