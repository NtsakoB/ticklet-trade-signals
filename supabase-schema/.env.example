# Supabase Configuration for Ticklet Trading Bot
# Copy this file to .env and fill in your actual values

# Supabase Project Settings
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database Configuration
DATABASE_URL=postgresql://postgres:[PASSWORD]@db.your-project-ref.supabase.co:5432/postgres

# Encryption Keys (for credential storage)
CREDENTIAL_ENCRYPTION_KEY=your-secure-encryption-key-here
MASTER_ENCRYPTION_KEY=your-master-key-for-additional-security

# Trading API Keys (these should be stored in Supabase secrets, not here)
# BINANCE_API_KEY=your-binance-api-key
# BINANCE_SECRET_KEY=your-binance-secret-key
# TELEGRAM_BOT_TOKEN=your-telegram-bot-token
# OPENAI_API_KEY=your-openai-api-key

# Edge Function Configuration
SUPABASE_FUNCTION_URL=https://your-project-ref.supabase.co/functions/v1

# Security Settings
JWT_SECRET=your-jwt-secret-here
AUTH_REDIRECT_URL=http://localhost:3000/auth/callback

# Development Settings
NODE_ENV=development
DEBUG_MODE=true
LOG_LEVEL=info

# Rate Limiting
API_RATE_LIMIT=1000
BINANCE_RATE_LIMIT=1200

# Trading Bot Configuration
DEFAULT_LEVERAGE=10
MAX_OPEN_TRADES=5
DEFAULT_RISK_PER_TRADE=2.0
DEFAULT_TRADING_MODE=paper

# Notification Settings
ENABLE_TELEGRAM_NOTIFICATIONS=true
ENABLE_EMAIL_NOTIFICATIONS=false
ENABLE_WEBHOOK_NOTIFICATIONS=false

# Webhook URLs (for external integrations)
WEBHOOK_SIGNAL_URL=https://your-webhook-endpoint.com/signals
WEBHOOK_TRADE_URL=https://your-webhook-endpoint.com/trades

# AI/ML Configuration
AI_MODEL_VERSION=v1.0
AI_CONFIDENCE_THRESHOLD=65.0
AI_LEARNING_RATE=0.001

# Backup and Recovery
BACKUP_ENABLED=true
BACKUP_FREQUENCY=daily
BACKUP_RETENTION_DAYS=30

# Instructions:
# 1. Copy this file to .env in your project root
# 2. Replace all placeholder values with your actual credentials
# 3. Never commit the .env file to version control
# 4. Use Supabase secrets management for sensitive API keys
# 5. Ensure proper file permissions (600) for the .env file